    "deploy:prod": "s3cmd sync --cf-invalidate -P -r dist/* s3://host.tkit.me",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-electron": "ng build && cp src/electron/* dist/",
    "electron": "npm run build-electron && electron dist/"
  },
